<?xml version="1.0"?>
<!--- This file sets default values to custom configuration options and this specifically enables the infocube documentation by default -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <infocube_docs>
            <general>
                <enable>1</enable>
            </general>
        </infocube_docs>
    </default>
</config>
